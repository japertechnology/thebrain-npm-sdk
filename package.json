{
  "name": "thebrain-api",
  "version": "0.0.1",
  "description": "TypeScript client for TheBrain API",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/index.min.js",
    "dist/index.min.esm.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn clean && yarn build",
    "test": "vitest --config vitest.config.ts --watch=false",
    "test:coverage": "vitest run --coverage --watch=false",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "thebrain",
    "api",
    "client",
    "typescript"
  ],
  "author": "Jalal",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "bunyan": "^1.8.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/bunyan": "^1.8.11",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitest/coverage-v8": "^3.1",
    "@vitest/ui": "^3.1",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.41.0",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "vitest": "^3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jalal/thebrain-api.git"
  },
  "bugs": {
    "url": "https://github.com/jalal/thebrain-api/issues"
  },
  "homepage": "https://github.com/jalal/thebrain-api#readme",
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
